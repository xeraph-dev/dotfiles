{
  "dashboard.projectData": null,
  "debug.openDebug": "openOnDebugBreak",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.hover.delay": 500,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.guides.bracketPairs": true,
  "editor.renderWhitespace": "trailing",
  "editor.insertSpaces": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "explorer.confirmDelete": false,
  "files.associations": {
    ".browserslistrc": "plaintext",
    ".postcssrc": "jsonc",
    "LICENSE": "plaintext"
  },
  "files.autoSave": "off",
  "files.exclude": {
    "package-lock.json": true,
    "yarn.lock": true,
    "pnpm-lock.yaml": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/node_modules/*/**": true,
    "*~": true
  },
  "files.watcherExclude": {
    "package-lock.json": true,
    "yarn.lock": true,
    "pnpm-lock.yaml": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "*~": true
  },
  "formattingToggle.affects": ["editor.formatOnSave"],
  "git.autofetch": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true,
    "typescript": true,
    "javascript": false,
    "haskell": false,
    "c": true,
    "sml": false,
    "hdl": false,
    "asm": false,
    "happy": false,
    "alex": false
  },
  "github.gitAuthentication": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.scopes": ["containers", "document"],
  "haskell.manageHLS": "GHCup",
  "haskell.ghcupExecutablePath": "~/.ghcup/bin/ghcup",
  "haskell.formattingProvider": "stylish-haskell",
  "haskell.upgradeGHCup": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "prettier.documentSelectors": ["**/*.astro"],
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.formatting.provider": "black",
  "quokka.showCodeLensInOutputChannel": false,
  "redhat.telemetry.enabled": false,
  "rest-client.enableTelemetry": false,
  "search.exclude": {
    "package-lock.json": true,
    "yarn.lock": true,
    "pnpm-lock.yaml": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "*~": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "none",
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Ayu Dark",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[haskell]": {
    "editor.defaultFormatter": "vigoo.stylish-haskell"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[astro]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}
